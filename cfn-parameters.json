{
	"Domain": "infra",
	"System": "backstage",
	"Component": $env.GITHUB_REPOSITORY_NAME_PART_SLUG_CS,
	"CodeBranch": $env.GITHUB_REF_SLUG_CS,
	"ClusterName": "/infra/backstage/backstage-cluster/main/EcsClusterName",
	"ClusterTaskIamRoleArn": "/infra/backstage/backstage-cluster/main/EcsTaskExecutionIamRoleArn",
	"ClusterSecurityGroupId": "/infra/backstage/backstage-cluster/main/ContainerSecurityGroupId",
	"ClusterAlbListenerArn": "/infra/backstage/backstage-cluster/main/EcsAlbListenerArn",
	"ContainerRepo": $secrets.AWS_ECR_REPO,
	"ContainerOrg": $env.GITHUB_REPOSITORY_OWNER_PART_SLUG,
	"ContainerImage": $env.GITHUB_REPOSITORY_NAME_PART_SLUG,
	"ContainerTag": $env.GITHUB_SHA,
	"ContainerCpu": "1024",
	"ContainerMemory": "2048",
	"ContainerDesiredCount": "1",
	"ContainerPort": "7007",
	"AlbRulePriority": "1",
	"AlbRulePath": "*",
	"VpcId": "/org/networking/VpcId",
	"VpcSubnets": "/org/networking/VpcPublicSubnets",
	"PgHost": $secrets.POSTGRES_HOST,
	"PgPort": "5432",
	"PgUser": $secrets.POSTGRES_USER,
	"PgPassword": $secrets.POSTGRES_PASSWORD,
	"AuthGoogleClientId": $secrets.AUTH_GOOGLE_CLIENT_ID,
	"AuthGoogleClientSecret": $secrets.AUTH_GOOGLE_CLIENT_SECRET,
	"DnsName": $secrets.DNS_NAME,
	"ServerlessOpsCatalogApiUrl": $vars.SERVERLESSOPS_CATALOG_API_URL,
	"ServerlessOpsAuthClientId": $secrets.SERVERLESSOPS_AUTH_CLIENT_ID,
	"ServerlessOpsAuthClientSecret": $secrets.SERVERLESSOPS_AUTH_CLIENT_SECRET,
	"ServerlessOpsAuthEndpoint": $secrets.SERVERLESSOPS_AUTH_ENDPOINT
}